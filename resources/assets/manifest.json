{"name": "watchlist",
 "version": "0.2.0",
 "web_accessible_resources": ["statusbar.html",
                              "images/watchlist-16.png"],
 "content_scripts": [{"matches": ["<all_urls>"],
                      "js":      ["compiled/watchlist.js"],
                      "css":     ["matches.css"],
                      "run_at":  "document_end"}],
 "options_ui": {"page": "options.html",
                "chrome_style": true},
 "icons": {"16":  "images/watchlist-16.png",
           "48":  "images/watchlist-48.png",
           "128": "images/watchlist-128.png"},
 "permissions": ["storage"],
 "manifest_version": 2}
